{
  "name": "tree-sitter-utl",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for UTL (Universal Template Language)",
  "main": "bindings/node",
  "keywords": [
    "tree-sitter",
    "parser",
    "utl",
    "template"
  ],
  "author": "Torstar Media",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8"
  },
  "tree-sitter": [
    {
      "scope": "source.utl",
      "file-types": [
        "utl"
      ],
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm"
    }
  ]
}
